static const uint32_t GPSBaud = 9600;

void setup()
{
  Serial.begin(GPSBaud);
  while (!Serial) {
  }
  Serial.print("cc1101_test2");
}

//-------------------------------------------------------------------------
void loop() 
{
  //-------------------------------------------------------------------------
  SerialRead();
  //-------------------------------------------------------------------------
}
static bool SerialRead()
{
  //-------------------------------------------------------------------------
  while (Serial.available())
  {
    char inChar = Serial.read();
    Serial.print(inChar);
  }
  //-------------------------------------------------------------------------
}
